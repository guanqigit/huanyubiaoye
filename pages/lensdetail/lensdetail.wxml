      </view>
      <view class='flex flex-center navigationbarbtn1'>
        <text>加入购镜单</text>
      </view>
      <view class='flex flex-center navigationbarbtn1 navigationbarbtn2'>
        <text>组合购买</text>
      </view>
    </view>
  </view>
</view>