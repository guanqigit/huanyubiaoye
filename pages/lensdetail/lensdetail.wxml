<view class="container">
  <view class='flex flex-column'>
    <view class='flex flex-center swiperbox'>
      <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-dots class='swiper' indicator-active-color="#4ce4ff" indicator-color="#fff" circular='true'>
        <block wx:for="{{imgUrls}}" wx:key=''>
          <swiper-item>
            <image src="{{item}}" class="slide-image" />
            <view class='videobtn flex flex-row flex-center'>
              <text>视频</text>
              <image src='../../image/righticon.png' class='playicon'></image>
            </view>
          </swiper-item>
        </block>
      </swiper>
    </view>
    <view class='header-info flex flex-column'>
      <view class='flex flex-center flex-row header-title'>
        <text>SUOFEIA-3267-C115</text>
        <text class='header-icon'>宽出</text>
      </view>
      <view class='flex flex-center flex-row header-name'>
        <text>修脸神器超大镜框钛金框防蓝光辐射眼镜架护目镜女</text>
      </view>
      <view class='flex flex-center flex-row header-pic'>
        <text>￥126</text>
      </view>
      <view class='flex flex-space header-content'>
        <text>好评率：88%</text>
        <text>邮费：包邮</text>
        <text>地址：上海</text>
      </view>
    </view>
    <view class='header-infos flex flex-column'>
      <view class='flex flex-space jkcolor-content'>
        <view class='flex flex-row flex-centers'>
          <image src='../../image/vouchericon.png' class='vouchericon'></image>
          <text>领取优惠券</text>
        </view>
        <view class='flex flex-center'>
          <image src='../../image/righticon.png' class='righticon'></image>
        </view>
      </view>
      <view class='flex flex-space jkcolor-contents'>
        <view class='flex flex-row flex-centers'>
          <image src='../../image/optometrydata.png' class='optometrydata'></image>
