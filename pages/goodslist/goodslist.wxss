  border-radius: 10rpx;
  border-bottom: 2rpx solid rgb(255, 255, 255);
  height: 54rpx;
}

.person_box {
  position: relative;
  top: 35rpx;
  left: -180rpx;
}

.phone_select {
  margin-top: 0;
  z-index: 100;
  position: absolute;
  background: #fff;
  width: 200rpx;
}

.select_one {
  text-align: center;
  background-color: #fff;
  height: 60rpx;
  line-height: 60rpx;
