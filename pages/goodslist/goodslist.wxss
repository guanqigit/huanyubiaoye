.header {
  padding: 20rpx 30rpx 30rpx;
  background: #fff;
}

.swiperbox {
  width: 176rpx;
  height: 54rpx;
  font-size: 24rpx;
  color: #222;
  position: relative;
  line-height: 54rpx;
  padding: 0 25rpx;
  background: #f5f7fa;
  border-radius: 2px;
  overflow: hidden;
}

.swiperbox:before {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: transparent;
  z-index: 1;
}

.searchbtn {
  width: 12rpx;
  height: 6rpx;
  position: absolute;
  top: 21rpx;
  right: 12rpx;
  z-index: 2;
}

.bottombtn {
  top: 30rpx;
  right: 12rpx;
}

.swiper {
  width: 100%;
  height: 100%;
}

.line {
  padding: 0 3%;
  height: 78rpx;
  background-color: #fff;
  border-radius: 4px;
  box-shadow: 0px 5px 18px 0px rgba(186, 186, 186, 0.38);
}

.search {
  width: 29rpx;
  height: 29rpx;
  margin-right: 13rpx;
  margin-left: 20rpx;
}

.searchinput {
