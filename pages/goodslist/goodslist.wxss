.personal_image {
  z-index: 100;
  position: absolute;
  right: 2.5%;
  width: 12rpx;
  height: 8rpx;
  margin: 25rpx 20rpx 40rpx 0;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

.rotateRight {
  transform: rotate(180deg);
}

